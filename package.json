{
  "name": "node-app",
  "version": "1.0.0",
  "description": "Basic Node.js, Express.js and MongoDb template application",
  "author": "Jeremy Thonon",
  "license": "ISC",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "start:prod": "NODE_ENV=production nodemon src/server.js",
    "debug": "node --inspect src/server.js",
    "lint": "eslint",
    "build": "webpack",
    "test": "node --test",
    "prettier": "prettier",
    "fill-database": "NODE_ENV=development node src/dev-data/import-dev-data.js --import",
    "clean-database": "NODE_ENV=development node src/dev-data/import-dev-data.js --delete"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.9.0",
    "mongoose": "^8.6.3",
    "nodemailer": "^6.9.16",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "type": "module",
  "imports": {
    "#src/*": "./src/*",
    "#controllers/*": "./src/controllers/*",
    "#middlewares/*": "./src/middlewares/*",
    "#models/*": "./src/models/*",
    "#routes/*": "./src/routes/*",
    "#public/*": "./src/templates/*",
    "#tests/*": "./src/tests/*",
    "#utils/*": "./src/utils/*"
  }
}
